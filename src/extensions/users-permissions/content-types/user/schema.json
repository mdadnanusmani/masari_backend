{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "likes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::like.like",
      "mappedBy": "user"
    },
    "mentions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::mention.mention",
      "mappedBy": "tgt_user"
    },
    "department": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::department.department"
    },
    "mobilePhone": {
      "type": "string"
    },
    "preferredLanguage": {
      "type": "string"
    },
    "userPrincipalName": {
      "type": "string"
    },
    "givenName": {
      "type": "string"
    },
    "displayName": {
      "type": "string"
    },
    "microsoft_id": {
      "type": "string"
    },
    "avatar": {
      "type": "text"
    },
    "roles": {
      "type": "text",
      "default": "authenticated,"
    },
    "cv": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::cv.cv"
    },
    "jobTitle": {
      "type": "string"
    },
    "acceptedTerms": {
      "type": "boolean"
    },
    "termsAcceptanceDate": {
      "type": "date"
    },
    "legalAcceptanceDate": {
      "type": "date"
    },
    "notifications": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::notification.notification",
      "inversedBy": "users_who_reads"
    },
    "comment": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::comment.comment",
      "inversedBy": "mentioned_users"
    },
    "Hr_Survey": {
      "type": "boolean",
      "default": false
    },
    "Ethical_Awareness": {
      "type": "boolean",
      "default": false
    },
    "process_role": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::process-role.process-role",
      "mappedBy": "user"
    },
    "requests": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::request.request",
      "mappedBy": "user"
    },
    "status_logs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::status-log.status-log",
      "mappedBy": "changed_by_user"
    },
    "request_adjustments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::request-adjustment.request-adjustment",
      "mappedBy": "adjusted_by_user"
    },
    "approvals": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::approval.approval",
      "mappedBy": "approved_by_user"
    },
    "cyber_security": {
      "type": "boolean",
      "default": false
    },
    "request_assigned": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::request.request",
      "mappedBy": "assigned_to"
    }
  }
}
